{% extends '::base.html.twig' %}

{% block body -%}
<div class="container">
    <h1>Zakup</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Czas</th>
                <td>{{ entity.czas|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>
<ul class="record_actions">
    <li>
        <a href="{{ path('buy') }}">
            Back to the list
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
</div>
{% endblock %}
